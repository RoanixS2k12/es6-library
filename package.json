    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-loader": "^1.3.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
